tmp="/tmp"
PID_FILE="$pwd/port-proxy.pid"
FRZ_FILE="$pwd/port-proxy.frz"
if [ -e $PID_FILE ]
then
	pid=`cat $PID_FILE`
	echo "[port-proxy] pid=$pid. Freez toggle.."
	kill -INT $pid
	if [ -e $FRZ_FILE ]
	then
		echo "[port-proxy] make unfreez...."
		rm $FRZ_FILE
	else
		echo "[port-proxy] make freez...."
		cp $PID_FILE $FRZ_FILE
	fi
	echo "[port-proxy] SIGINT done"
else
	echo "[port-proxy] No process to Freez..."

fi

