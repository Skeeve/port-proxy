tmp="/tmp"
PID_FILE="$pwd/port-proxy.pid"
FRZ_FILE="$pwd/port-proxy.frz"

if [ -e $PID_FILE ]
then
	pid=`cat $PID_FILE`
	echo "[port-proxy] pid=$pid. stopping.."
	kill -9 $pid
	rm $PID_FILE
	if [ -e $FRZ_FILE ]
	then
 		rm $FRZ_FILE
	fi
	echo "[port-proxy] stop done"
else
	echo "[port-proxy] No process to stop..."

fi

